{
    "rewrites": [
      {
        "source": "/auth/(.*)", 
        "destination": "https://dominio.app/auth/$1"
      },
      {
        "source": "/user/(.*)", 
        "destination": "https://dominio.app/api/$1"
      },
      {
        "source": "/post/(.*)", 
        "destination": "https://dominio.app/api/$1"
      },
      {
        "source": "/(.*)", 
        "destination": "/index.html"
      }
    ]
  }